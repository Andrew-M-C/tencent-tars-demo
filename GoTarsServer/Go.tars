module amc {
    struct GetTimeReq
    {
        0 optional  string  time_fmt;
    };

    struct GetTimeRsp
    {
        0 require   long    utc_timestamp;  // UTC UNIX timestamp
        1 require   long    local_timestamp;
        2 require   string  local_time_str;
    };

    interface Go
    {
        int GetTime(GetTimeReq req, out GetTimeRsp rsp);
    };
};
